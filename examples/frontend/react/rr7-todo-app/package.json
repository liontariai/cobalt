{
    "name": "rr7-todo-app",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "rr7:build": "react-router build",
        "build": "concurrently  \"cobalt build\" \"bun run rr7:build\"",
        "rr7:dev": "react-router dev --force",
        "dev": "concurrently --restartTries 10 --restartDelay 2000 \"cobalt dev\" \"bun run rr7:dev\"",
        "rr7:start": "react-router-serve ./build/server/index.js",
        "start": "concurrently --restartTries 10 --restartDelay 2000 \"cobalt start\" \"bun run rr7:start\"",
        "typecheck": "react-router typegen && tsc",
        "cobalt:dev": "cobalt dev",
        "cobalt:build": "cobalt build",
        "cobalt:start": "cobalt start",
        "postinstall": "prisma generate && prisma db push",
        "db:push": "prisma db push",
        "prettier": "prettier --write ."
    },
    "dependencies": {
        "@cobalt27/auth": "latest",
        "@cobalt27/dev": "latest",
        "@cobalt27/runtime": "latest",
        "@graphql-tools/schema": "^10.0.0",
        "@prisma/client": "^6.14.0",
        "@react-router/node": "^7.5.3",
        "@react-router/serve": "^7.5.3",
        "framer-motion": "^12.23.12",
        "graphql": "^16.8.1",
        "graphql-sse": "^2.5.4",
        "isbot": "^5.1.27",
        "prisma": "^6.14.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router": "^7.5.3",
        "react-router-dom": "^7.7.1",
        "swr": "^2.3.6",
        "valibot": "1.1.0"
    },
    "devDependencies": {
        "@react-router/dev": "^7.5.3",
        "@tailwindcss/vite": "^4.1.4",
        "@types/node": "^20",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "bun-types": "latest",
        "concurrently": "^9.2.0",
        "prettier": "latest",
        "tailwindcss": "^4.1.4",
        "typescript": "^5.8.3",
        "vite": "^6.3.3",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
